  "type": "module",
  "engines": {
    "node": "14.17.x"
  },
  "dependencies": {
    "findit": "2.0.0",
    "fs": "0.0.1-security",
    "inquirer": "8.1.5",
    "prompt": "1.2.0",
    "readline": "1.3.0",
    "replace-once": "1.0.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/runtime": "^7.14.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.1.0",
    "jscpd": "^3.3.26",
    "prettier": "^2.3.2"
  },
  "scripts": {
    "install_prod_modules": "yarn install --production --frozen-lockfile",
    "setup": "node src/setup.js",
    "deploy_docker": "cd cloned_projects && docker-compose --env-file ./docker.env up -d",
    "api_run_dev": "cd cloned_projects/api && yarn dev",
    "frontend_run_dev": "cd cloned_projects/frontend && yarn dev",
    "rebuild_frontend_docker": "cd cloned_projects && docker-compose --env-file ./docker.env up -d --no-deps --build frontend",
    "rebuild_api_docker": "cd cloned_projects && docker-compose --env-file ./docker.env up -d --no-deps --build api"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 110
  }
